    'DiamondBackgroundColor #007ac9
    DiamondBackgroundColor STYLE_DEFAULT_WORK_LITE
    DiamondBackgroundColor<<SCA>> #STYLE_DEFAULT_SCA_LITE
    DiamondBackgroundColor<<External>> #STYLE_DEFAULT_EXTERNAL_LITE

    BorderColor STYLE_DEFAULT_BORDER
    FontColor STYLE_DEFAULT_WORK
