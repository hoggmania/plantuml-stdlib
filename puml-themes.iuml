@startuml


!define STYLE_DEFAULT_WORK #ffffff
!define STYLE_DEFAULT_WORK_LITE #LightBlue

!define STYLE_DEFAULT_SCA #DarkGreen
!define STYLE_DEFAULT_SCA_LITE #LightGreen

!define STYLE_DEFAULT_EXTERNAL #Orange
!define STYLE_DEFAULT_EXTERNAL_LITE #LightOrange

!define STYLE_DEFAULT_BORDER #Black



skinparam SequenceGroupBodyBackgroundColor transparent 
skinparam ParticipantPadding 0
skinparam BoxPadding 10
skinparam shadowing false
skinparam roundcorner 20
skinparam BackgroundColor STYLE_DEFAULT_WORK
skinparam ArrowColor STYLE_DEFAULT_BORDER



skinparam default {
    BackgroundColor STYLE_DEFAULT_WORK_LITE
    BackgroundColor<<SCA>> #STYLE_DEFAULT_SCA_LITE
    BackgroundColor<<External>> #STYLE_DEFAULT_EXTERNAL_LITE
    BorderColor STYLE_DEFAULT_BORDER
}

